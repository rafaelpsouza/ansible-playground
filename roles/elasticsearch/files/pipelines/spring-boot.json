{
  "description" : "Ingest pipeline for Spring Boot Apps",
  "processors" : [
    {
      "grok": {
        "field": "message",
        "patterns": ["(?<timestamp>%{YEAR}-%{MONTHNUM}-%{MONTHDAY} %{TIME})\\s+%{LOGLEVEL:level}\\s+%{NUMBER:pid}\\s+---\\s+\\[\\s*%{DATA:thread}\\s*\\]\\s+%{DATA:class}\\s*:\\s*%{DATA:message}$"]
      }
    }
  ]
}